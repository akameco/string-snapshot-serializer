{
  "name": "string-snapshot-serializer",
  "version": "1.0.0",
  "description": "serializer for string escape",
  "license": "MIT",
  "repository": "akameco/string-snapshot-serializer",
  "author": "akameco <akameco.t@gmail.com> (https://akameco.github.io)",
  "files": [
    "dest"
  ],
  "main": "dest/index.js",
  "types": "dest/index.d.ts",
  "keywords": [
    "jest",
    "snapshot",
    "serializer"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "contributors:add": "all-contributors add",
    "build": "del dest && tsc",
    "prepublish": "yarn build && del dest/**/*.test.*",
    "fmt": "prettier --write '**/*.{ts,js,json,md}'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:ci": "jest --coverage --ci --runInBand"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@akameco/tsconfig": "^0.1.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@typescript-eslint/parser": "^1.3.0",
    "all-contributors-cli": "^6.1.1",
    "del-cli": "^1.1.0",
    "eslint": "^5.13.0",
    "eslint-config-precure": "^4.3.3",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3"
  }
}
